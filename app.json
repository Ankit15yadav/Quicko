{
  "expo": {
    "name": "quicko-frontend",
    "slug": "quicko-frontend",
    "icon": "./src/assets/images/quicko-logo.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "quickofrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./src/assets/ios-dark.png",
        "light": "./src/assets/ios-dark.png",
        "tinted": "./src/assets/ios-dark.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/blinkit.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#242424",
          "dark": {
            "image": "./src/assets/images/blinkit.svg"
          }
        }
      ],
      [
         "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Quicko to use your location.",
          
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
